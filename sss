local tabButtons = {}
local currentTab = "home"

-- Hƒ∞LE DEƒûƒ∞≈ûKENLERƒ∞
local flying = false
local noclip = false
local godmode = false
local speedBoost = false
local jumpPower = false
local infiniteJump = false
local espEnabled = false
local bodyVelocity
local bodyGyro
local toggleButtons = {}

-- Modern fonksiyonlar
function createTabFrame(tabId)
    local frame = Instance.new("Frame")
    frame.Size = UDim2.new(1, 0, 1, 0)
    frame.Position = UDim2.new(0, 0, 0, 0)
    frame.BackgroundTransparency = 1
    frame.Visible = false
    frame.Parent = contentFrame
    frame.Name = tabId
    return frame
end

function createScrollFrame(parent)
    local scroll = Instance.new("ScrollingFrame")
    scroll.Size = UDim2.new(1, 0, 1, 0)
    scroll.Position = UDim2.new(0, 0, 0, 0)
    scroll.BackgroundTransparency = 1
    scroll.ScrollBarThickness = 6
    scroll.ScrollBarImageColor3 = Colors.Border
    scroll.ScrollBarImageTransparency = 0.5
    scroll.CanvasSize = UDim2.new(0, 0, 0, 0)
    scroll.AutomaticCanvasSize = Enum.AutomaticSize.Y
    scroll.Parent = parent
    
    local uiPadding = Instance.new("UIPadding")
    uiPadding.PaddingTop = UDim.new(0, 10)
    uiPadding.PaddingLeft = UDim.new(0, 10)
    uiPadding.Parent = scroll
    
    return scroll
end

function createToggleButton(name, desc, parent, category)
    local frame = Instance.new("Frame")
    frame.Size = UDim2.new(1, -20, 0, 90)
    frame.BackgroundColor3 = Colors.Secondary
    frame.BorderSizePixel = 0
    frame.Parent = parent
    frame.Name = name
    
    local corner = Instance.new("UICorner")
    corner.CornerRadius = UDim.new(0, 12)
    corner.Parent = frame
    
    local stroke = Instance.new("UIStroke")
    stroke.Color = Colors.Border
    stroke.Thickness = 1
    stroke.Parent = frame
    
    -- Hover efekti
    local hover = Instance.new("Frame")
    hover.Size = UDim2.new(1, 0, 1, 0)
    hover.BackgroundColor3 = Colors.TextPrimary
    hover.BackgroundTransparency = 0.95
    hover.BorderSizePixel = 0
    hover.Visible = false
    hover.Parent = frame
    
    local hoverCorner = Instance.new("UICorner")
    hoverCorner.CornerRadius = UDim.new(0, 12)
    hoverCorner.Parent = hover
    
    -- Ba≈ülƒ±k
    local titleLabel = Instance.new("TextLabel")
    titleLabel.Size = UDim2.new(0.7, 0, 0, 25)
    titleLabel.Position = UDim2.new(0, 15, 0, 12)
    titleLabel.BackgroundTransparency = 1
    titleLabel.TextColor3 = Colors.TextPrimary
    titleLabel.Text = name
    titleLabel.TextSize = 16
    titleLabel.Font = Enum.Font.GothamBold
    titleLabel.TextXAlignment = Enum.TextXAlignment.Left
    titleLabel.Parent = frame
    
    -- A√ßƒ±klama
    local descLabel = Instance.new("TextLabel")
    descLabel.Size = UDim2.new(0.7, -10, 0, 40)
    descLabel.Position = UDim2.new(0, 15, 0, 35)
    descLabel.BackgroundTransparency = 1
    descLabel.TextColor3 = Colors.TextSecondary
    descLabel.Text = desc
    descLabel.TextSize = 12
    descLabel.Font = Enum.Font.Gotham
    descLabel.TextXAlignment = Enum.TextXAlignment.Left
    descLabel.TextYAlignment = Enum.TextYAlignment.Top
    descLabel.TextWrapped = true
    descLabel.Parent = frame
    
    -- Toggle Butonu
    local toggleButton = Instance.new("TextButton")
    toggleButton.Size = UDim2.new(0, 100, 0, 38)
    toggleButton.Position = UDim2.new(1, -120, 0, 25)
    toggleButton.BackgroundColor3 = Color3.fromRGB(48, 54, 61)
    toggleButton.TextColor3 = Colors.TextPrimary
    toggleButton.Text = "KAPALI"
    toggleButton.TextSize = 12
    toggleButton.Font = Enum.Font.GothamBold
    toggleButton.AutoButtonColor = false
    toggleButton.Parent = frame
    
    local toggleCorner = Instance.new("UICorner")
    toggleCorner.CornerRadius = UDim.new(0, 8)
    toggleCorner.Parent = toggleButton
    
    local toggleStroke = Instance.new("UIStroke")
    toggleStroke.Color = Colors.Border
    toggleStroke.Thickness = 2
    toggleStroke.Parent = toggleButton
    
    -- Hover efektleri
    frame.MouseEnter:Connect(function()
        hover.Visible = true
    end)
    
    frame.MouseLeave:Connect(function()
        hover.Visible = false
    end)
    
    toggleButton.MouseEnter:Connect(function()
        TweenService:Create(toggleButton, TweenInfo.new(0.2), {BackgroundColor3 = Color3.fromRGB(58, 64, 71)}):Play()
    end)
    
    toggleButton.MouseLeave:Connect(function()
        local data = toggleButtons[name]
        if data and not data.enabled then
            TweenService:Create(toggleButton, TweenInfo.new(0.2), {BackgroundColor3 = Color3.fromRGB(48, 54, 61)}):Play()
        end
    end)
    
    -- Toggle'ƒ± kaydet
    toggleButtons[name] = {
        button = toggleButton,
        enabled = false,
        category = category,
        name = name
    }
    
    -- Click event
    toggleButton.MouseButton1Click:Connect(function()
        toggleFeature(name)
    end)
    
    return frame
end

function toggleFeature(name)
    local data = toggleButtons[name]
    if not data then return end
    
    data.enabled = not data.enabled
    
    if data.enabled then
        -- A√ßƒ±k animasyonu
        TweenService:Create(data.button, TweenInfo.new(0.3), {
            BackgroundColor3 = Colors.Success,
            Text = "AKTƒ∞F"
        }):Play()
        activateFeature(name)
    else
        -- Kapalƒ± animasyonu
        TweenService:Create(data.button, TweenInfo.new(0.3), {
            BackgroundColor3 = Color3.fromRGB(48, 54, 61),
            Text = "KAPALI"
        }):Play()
        deactivateFeature(name)
    end
end

-- Hƒ∞LE AKTƒ∞VASYON FONKSƒ∞YONLARI
function activateFeature(name)
    if name == "Fly Mode" then
        flying = true
        local character = player.Character
        if character then
            local root = character:FindFirstChild("HumanoidRootPart")
            if root then
                bodyVelocity = Instance.new("BodyVelocity")
                bodyVelocity.Velocity = Vector3.new(0, 0, 0)
                bodyVelocity.MaxForce = Vector3.new(40000, 40000, 40000)
                bodyVelocity.Parent = root
                
                bodyGyro = Instance.new("BodyGyro")
                bodyGyro.MaxTorque = Vector3.new(50000, 50000, 50000)
                bodyGyro.P = 10000
                bodyGyro.D = 1000
                bodyGyro.CFrame = root.CFrame
                bodyGyro.Parent = root
            end
        end
        print("üöÄ Fly Mode: AKTƒ∞F - W,A,S,D, Space, Shift ile u√ß!")
    elseif name == "NoClip Mode" then
        noclip = true
        print("üåÄ NoClip Mode: AKTƒ∞F - Duvarlardan ge√ßebilirsin!")
    elseif name == "God Mode" then
        godmode = true
        local character = player.Character
        if character then
            local humanoid = character:FindFirstChildOfClass("Humanoid")
            if humanoid then
                humanoid.MaxHealth = math.huge
                humanoid.Health = math.huge
            end
        end
        print("üõ°Ô∏è God Mode: AKTƒ∞F - √ñl√ºms√ºz oldun!")
    elseif name == "Speed Boost" then
        speedBoost = true
        local character = player.Character
        if character then
            local humanoid = character:FindFirstChildOfClass("Humanoid")
            if humanoid then
                humanoid.WalkSpeed = 50
            end
        end
        print("üí® Speed Boost: AKTƒ∞F - Hƒ±zƒ±n arttƒ±!")
    elseif name == "Infinite Jump" then
        infiniteJump = true
        print("ü¶ò Infinite Jump: AKTƒ∞F - Sonsuz zƒ±plama!")
    elseif name == "High Jump" then
        jumpPower = true
        local character = player.Character
        if character then
            local humanoid = character:FindFirstChildOfClass("Humanoid")
            if humanoid then
                humanoid.JumpPower = 100
            end
        end
        print("üèÄ High Jump: AKTƒ∞F - Y√ºksek zƒ±plama!")
    elseif name == "Player ESP" then
        espEnabled = true
        for _, otherPlayer in pairs(Players:GetPlayers()) do
            if otherPlayer ~= player and otherPlayer.Character then
                local highlight = Instance.new("Highlight")
                highlight.FillColor = Color3.new(1, 0, 0)
                highlight.OutlineColor = Color3.new(1, 1, 0)
                highlight.FillTransparency = 0.5
                highlight.OutlineTransparency = 0
                highlight.Parent = otherPlayer.Character
            end
        end
        print("üëÅÔ∏è Player ESP: AKTƒ∞F - Oyuncularƒ± g√∂r√ºyorsun!")
    end
end

function deactivateFeature(name)
    if name == "Fly Mode" then
        flying = false
        if bodyVelocity then bodyVelocity:Destroy() bodyVelocity = nil end
        if bodyGyro then bodyGyro:Destroy() bodyGyro = nil end
        print("üöÄ Fly Mode: DEVRE DI≈ûI")
    elseif name == "NoClip Mode" then
        noclip = false
        print("üåÄ NoClip Mode: DEVRE DI≈ûI")
    elseif name == "God Mode" then
        godmode = false
        local character = player.Character
        if character then
            local humanoid = character:FindFirstChildOfClass("Humanoid")
            if humanoid then
                humanoid.MaxHealth = 100
                humanoid.Health = 100
            end
        end
        print("üõ°Ô∏è God Mode: DEVRE DI≈ûI")
    elseif name == "Speed Boost" then
        speedBoost = false
        local character = player.Character
        if character then
            local humanoid = character:FindFirstChildOfClass("Humanoid")
            if humanoid then
                humanoid.WalkSpeed = 16
            end
        end
        print("üí® Speed Boost: DEVRE DI≈ûI")
    elseif name == "Infinite Jump" then
        infiniteJump = false
        print("ü¶ò Infinite Jump: DEVRE DI≈ûI")
    elseif name == "High Jump" then
        jumpPower = false
        local character = player.Character
        if character then
            local humanoid = character:FindFirstChildOfClass("Humanoid")
            if humanoid then
                humanoid.JumpPower = 50
            end
        end
        print("üèÄ High Jump: DEVRE DI≈ûI")
    elseif name == "Player ESP" then
        espEnabled = false
        for _, otherPlayer in pairs(Players:GetPlayers()) do
            if otherPlayer.Character then
                local highlight = otherPlayer.Character:FindFirstChild("Highlight")
                if highlight then
                    highlight:Destroy()
                end
            end
        end
        print("üëÅÔ∏è Player ESP: DEVRE DI≈ûI")
    end
end

function switchTab(tabId)
    currentTab = tabId
    
    -- T√ºm sekmeleri gizle
    for _, tab in ipairs(tabs) do
        local frame = contentFrame:FindFirstChild(tab.id)
        if frame then
            frame.Visible = false
        end
        if tabButtons[tab.id] then
            TweenService:Create(tabButtons[tab.id], TweenInfo.new(0.3), {
                BackgroundColor3 = Colors.Secondary,
                TextColor3 = Colors.TextSecondary
            }):Play()
        end
    end
    
    -- Aktif sekme
    local activeFrame = contentFrame:FindFirstChild(tabId)
    if activeFrame then
        activeFrame.Visible = true
    end
    if tabButtons[tabId] then
        TweenService:Create(tabButtons[tabId], TweenInfo.new(0.3), {
            BackgroundColor3 = Colors.Accent,
            TextColor3 = Colors.TextPrimary
        }):Play()
    end
end

-- Modern sekme butonlarƒ± olu≈ütur
for i, tab in ipairs(tabs) do
    local tabButton = Instance.new("TextButton")
    tabButton.Size = UDim2.new(1, -15, 0, 52)
    tabButton.Position = UDim2.new(0, 7, 0, 15 + (i * 60))
    tabButton.BackgroundColor3 = Colors.Secondary
    tabButton.TextColor3 = Colors.TextSecondary
    tabButton.Text = "  " .. tab.icon .. "  " .. tab.name
    tabButton.TextSize = 14
    tabButton.Font = Enum.Font.Gotham
    tabButton.TextXAlignment = Enum.TextXAlignment.Left
    tabButton.AutoButtonColor = false
    tabButton.Parent = tabFrame
    
    local tabCorner = Instance.new("UICorner")
    tabCorner.CornerRadius = UDim.new(0, 10)
    tabCorner.Parent = tabButton
    
    local tabStroke = Instance.new("UIStroke")
    tabStroke.Color = Colors.Border
    tabStroke.Thickness = 1
    tabStroke.Parent = tabButton
    
    -- Hover efekti
    tabButton.MouseEnter:Connect(function()
        if currentTab ~= tab.id then
            TweenService:Create(tabButton, TweenInfo.new(0.2), {
                BackgroundColor3 = Color3.fromRGB(28, 33, 40)
            }):Play()
        end
    end)
    
    tabButton.MouseLeave:Connect(function()
        if currentTab ~= tab.id then
            TweenService:Create(tabButton, TweenInfo.new(0.2), {
                BackgroundColor3 = Colors.Secondary
            }):Play()
        end
    end)
    
    tabButtons[tab.id] = tabButton
    
    tabButton.MouseButton1Click:Connect(function()
        switchTab(tab.id)
    end)
end

