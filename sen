    descLabel.Position = UDim2.new(0, 15, 0, 37)
    descLabel.BackgroundTransparency = 1
    descLabel.TextColor3 = Colors.TextSecondary
    descLabel.Text = desc
    descLabel.TextSize = 12
    descLabel.Font = Enum.Font.Gotham
    descLabel.TextXAlignment = Enum.TextXAlignment.Left
    descLabel.Parent = frame
    
    -- Oyuncu adƒ± input
    local playerInput = Instance.new("TextBox")
    playerInput.Size = UDim2.new(0.65, 0, 0, 36)
    playerInput.Position = UDim2.new(0, 15, 0, 62)
    playerInput.BackgroundColor3 = Color3.fromRGB(13, 17, 23)
    playerInput.TextColor3 = Colors.TextPrimary
    playerInput.PlaceholderText = "Oyuncu adƒ± yaz..."
    playerInput.PlaceholderColor3 = Colors.TextSecondary
    playerInput.Text = ""
    playerInput.TextSize = 12
    playerInput.Font = Enum.Font.Gotham
    playerInput.Parent = frame
    
    local inputCorner = Instance.new("UICorner")
    inputCorner.CornerRadius = UDim.new(0, 8)
    inputCorner.Parent = playerInput
    
    local inputStroke = Instance.new("UIStroke")
    inputStroke.Color = Colors.Border
    inputStroke.Thickness = 2
    inputStroke.Parent = playerInput
    
    -- Teleport Butonu
    local teleportButton = Instance.new("TextButton")
    teleportButton.Size = UDim2.new(0, 130, 0, 36)
    teleportButton.Position = UDim2.new(1, -155, 0, 62)
    teleportButton.BackgroundColor3 = Colors.Accent
    teleportButton.TextColor3 = Colors.TextPrimary
    teleportButton.Text = "üöÄ I≈üƒ±nlan"
    teleportButton.TextSize = 12
    teleportButton.Font = Enum.Font.GothamBold
    teleportButton.Parent = frame
    
    local buttonCorner = Instance.new("UICorner")
    buttonCorner.CornerRadius = UDim.new(0, 8)
    buttonCorner.Parent = teleportButton
    
    local buttonStroke = Instance.new("UIStroke")
    buttonStroke.Color = Color3.fromRGB(100, 150, 255)
    buttonStroke.Thickness = 2
    buttonStroke.Parent = teleportButton
    
    -- Hover efekti
    teleportButton.MouseEnter:Connect(function()
        TweenService:Create(teleportButton, TweenInfo.new(0.2), {BackgroundColor3 = Color3.fromRGB(20, 120, 220)}):Play()
    end)
    
    teleportButton.MouseLeave:Connect(function()
        TweenService:Create(teleportButton, TweenInfo.new(0.2), {BackgroundColor3 = Colors.Accent}):Play()
    end)
    
    -- Teleport fonksiyonu
    teleportButton.MouseButton1Click:Connect(function()
        local targetName = playerInput.Text
        if targetName and targetName ~= "" then
            teleportToPlayer(targetName)
        else
            local originalText = teleportButton.Text
            teleportButton.Text = "‚ùå ƒ∞sim gir!"
            teleportButton.BackgroundColor3 = Colors.Danger
            wait(1.5)
            teleportButton.Text = originalText
            teleportButton.BackgroundColor3 = Colors.Accent
        end
    end)
    
    return frame
end

-- Hƒ∞LE FONKSƒ∞YONLARI
function teleportToPlayer(playerName)
    local targetPlayer = findPlayer(playerName)
    if targetPlayer and targetPlayer.Character then
        local targetRoot = targetPlayer.Character:FindFirstChild("HumanoidRootPart")
        local myCharacter = player.Character
        local myRoot = myCharacter and myCharacter:FindFirstChild("HumanoidRootPart")
        
        if targetRoot and myRoot then
            myRoot.CFrame = targetRoot.CFrame + Vector3.new(0, 3, 0)
            print("‚úÖ " .. targetPlayer.Name .. " adlƒ± oyuncuya ƒ±≈üƒ±nlanƒ±ldƒ±!")
        else
            print("‚ùå Hedef oyuncu bulunamadƒ±!")
        end
    else
        print("‚ùå Oyuncu bulunamadƒ±: " .. playerName)
    end
end

function findPlayer(name)
    name = name:lower()
    for _, plr in pairs(Players:GetPlayers()) do
        if plr.Name:lower():find(name) or plr.DisplayName:lower():find(name) then
            return plr
        end
    end
    return nil
end

function toggleFeature(name)
    local data = toggleButtons[name]
    if not data then return end
    
    data.enabled = not data.enabled
    
    if data.enabled then
        -- A√ßƒ±k animasyonu
        TweenService:Create(data.button, TweenInfo.new(0.3), {
            BackgroundColor3 = Colors.Success,
            Text = "AKTƒ∞F"
        }):Play()
        activateFeature(name)
    else
        -- Kapalƒ± animasyonu
        TweenService:Create(data.button, TweenInfo.new(0.3), {
            BackgroundColor3 = Color3.fromRGB(48, 54, 61),
            Text = "KAPALI"
        }):Play()
        deactivateFeature(name)
    end
end

-- LAN Hƒ∞LE FONKSƒ∞YONLARI
function executeLanFeature(name)
    if name == "Kill All Players" then
        killAllPlayers()
    elseif name == "Bring All Players" then
        bringAllPlayers()
    elseif name == "Freeze All Players" then
        freezeAllPlayers()
    elseif name == "Spam Message" then
        spamMessageToAll()
    elseif name == "Crash Server" then
        crashServer()
    elseif name == "Remove All Tools" then
        removeAllTools()
    elseif name == "Lag Server" then
        lagServer()
    end
end

function killAllPlayers()
    for _, otherPlayer in pairs(Players:GetPlayers()) do
        if otherPlayer ~= player and otherPlayer.Character then
            local humanoid = otherPlayer.Character:FindFirstChildOfClass("Humanoid")
            if humanoid then
                humanoid.Health = 0
            end
        end
    end
    print("üíÄ T√ºm oyuncular √∂ld√ºr√ºld√º!")
end

function bringAllPlayers()
    local myCharacter = player.Character
    local myRoot = myCharacter and myCharacter:FindFirstChild("HumanoidRootPart")
    
    if myRoot then
        for _, otherPlayer in pairs(Players:GetPlayers()) do
            if otherPlayer ~= player and otherPlayer.Character then
                local targetRoot = otherPlayer.Character:FindFirstChild("HumanoidRootPart")
                if targetRoot then
                    targetRoot.CFrame = myRoot.CFrame + Vector3.new(math.random(-5, 5), 0, math.random(-5, 5))
                end
            end
        end
        print("üéØ T√ºm oyuncular yanƒ±na getirildi!")
    end
end

function freezeAllPlayers()
    for _, otherPlayer in pairs(Players:GetPlayers()) do
        if otherPlayer ~= player and otherPlayer.Character then
            for _, part in pairs(otherPlayer.Character:GetDescendants()) do
                if part:IsA("BasePart") then
                    part.Anchored = true
                end
            end
        end
    end
    print("‚ùÑÔ∏è T√ºm oyuncular donduruldu!")
end

function spamMessageToAll()
    local messages = {
        "ULTIMATE ADMIN AKTƒ∞F! üöÄ",
        "LAN Hileleri √áalƒ±≈üƒ±yor! üí•",
        "Admin Modu AKTƒ∞F! üëë",
        "Ultimate Admin ile g√º√ß sende! ‚ö°"
    }
    
    for i = 1, 5 do
        local message = messages[math.random(1, #messages)]
        game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(message, "All")
        wait(1)
    end
    print("üì¢ Spam mesajlarƒ± g√∂nderildi!")
end

function crashServer()
    while true do
        for i = 1, 100 do
            local part = Instance.new("Part")
            part.Parent = workspace
            part.Position = Vector3.new(math.random(-100, 100), math.random(10, 50), math.random(-100, 100))
        end
        wait(0.1)
    end
    print("üí• Sunucu √ß√∂kertiliyor...")
end

function removeAllTools()
    for _, otherPlayer in pairs(Players:GetPlayers()) do
        if otherPlayer ~= player and otherPlayer.Backpack then
            for _, tool in pairs(otherPlayer.Backpack:GetChildren()) do
                if tool:IsA("Tool") then
                    tool:Destroy()
                end
            end
        end
        if otherPlayer.Character then
            for _, tool in pairs(otherPlayer.Character:GetChildren()) do
                if tool:IsA("Tool") then
                    tool:Destroy()
                end
            end
        end
    end
    print("üîß T√ºm oyuncularƒ±n ekipmanlarƒ± silindi!")
end

function lagServer()
    for i = 1, 50 do
        local script = Instance.new("Script")
        script.Parent = workspace
        coroutine.wrap(function()
            while true do
                wait()
            end
        end)()
    end
    print("üêå Sunucu laglatƒ±lƒ±yor...")
end

-- ADMIN KOMUT FONKSƒ∞YONLARI
function executeAdminCommand(name)
    if name == "Fly A√ß" then
        toggleFeature("Fly Mode")
    elseif name == "NoClip A√ß" then
        toggleFeature("NoClip Mode")
    elseif name == "God Mode A√ß" then
        toggleFeature("God Mode")
    elseif name == "Speed Boost A√ß" then
        toggleFeature("Speed Boost")
    elseif name == "Infinite Jump A√ß" then
        toggleFeature("Infinite Jump")
    elseif name == "High Jump A√ß" then
        toggleFeature("High Jump")
    elseif name == "ESP A√ß" then
        toggleFeature("Player ESP")
    elseif name == "Invisibility A√ß" then
        toggleFeature("Invisibility")
    elseif name == "T√ºm Hileleri A√ß" then
        enableAllHacks()
    elseif name == "T√ºm Hileleri Kapat" then
        disableAllHacks()
    end
end

function enableAllHacks()
    for name, data in pairs(toggleButtons) do
        if not data.enabled then
            toggleFeature(name)
        end
    end
    print("üéØ T√ºm hileler AKTƒ∞F edildi!")
end

function disableAllHacks()
    for name, data in pairs(toggleButtons) do
        if data.enabled then
            toggleFeature(name)
        end
    end
    print("üî¥ T√ºm hileler KAPATILDI!")
end

-- Hƒ∞LE AKTƒ∞VASYON FONKSƒ∞YONLARI
function activateFeature(name)
    if name == "Fly Mode" then
        flying = true
        local character = player.Character
        if character then
            local root = character:FindFirstChild("HumanoidRootPart")
            if root then
                bodyVelocity = Instance.new("BodyVelocity")
                bodyVelocity.Velocity = Vector3.new(0, 0, 0)
                bodyVelocity.MaxForce = Vector3.new(40000, 40000, 40000)
                bodyVelocity.Parent = root
                
                bodyGyro = Instance.new("BodyGyro")
                bodyGyro.MaxTorque = Vector3.new(50000, 50000, 50000)
                bodyGyro.P = 10000
                bodyGyro.D = 1000
                bodyGyro.CFrame = root.CFrame
                bodyGyro.Parent = root
            end
        end
        print("üöÄ Fly Mode: AKTƒ∞F - W,A,S,D, Space, Shift ile u√ß!")
    elseif name == "NoClip Mode" then
        noclip = true
        print("üåÄ NoClip Mode: AKTƒ∞F - Duvarlardan ge√ßebilirsin!")
    elseif name == "God Mode" then
        godmode = true
        local character = player.Character
        if character then
            local humanoid = character:FindFirstChildOfClass("Humanoid")
            if humanoid then
                humanoid.MaxHealth = math.huge
                humanoid.Health = math.huge
            end
        end
        print("üõ°Ô∏è God Mode: AKTƒ∞F - √ñl√ºms√ºz oldun!")
    elseif name == "Speed Boost" then
        speedBoost = true
        local character = player.Character
        if character then
            local humanoid = character:FindFirstChildOfClass("Humanoid")
            if humanoid then
                humanoid.WalkSpeed = 50
            end
        end
        print("üí® Speed Boost: AKTƒ∞F - Hƒ±zƒ±n arttƒ±!")
    elseif name == "Infinite Jump" then
        infiniteJump = true
        print("ü¶ò Infinite Jump: AKTƒ∞F - Sonsuz zƒ±plama!")
    elseif name == "High Jump" then
        jumpPower = true
        local character = player.Character
        if character then
            local humanoid = character:FindFirstChildOfClass("Humanoid")
            if humanoid then
                humanoid.JumpPower = 100
            end
        end
        print("üèÄ High Jump: AKTƒ∞F - Y√ºksek zƒ±plama!")
    elseif name == "Player ESP" then
        for _, otherPlayer in pairs(Players:GetPlayers()) do
            if otherPlayer ~= player and otherPlayer.Character then
                local highlight = Instance.new("Highlight")
                highlight.FillColor = Color3.new(1, 0, 0)
                highlight.OutlineColor = Color3.new(1, 1, 0)
                highlight.FillTransparency = 0.5
                highlight.OutlineTransparency = 0
                highlight.Parent = otherPlayer.Character
            end
        end
        print("üëÅÔ∏è Player ESP: AKTƒ∞F - Oyuncularƒ± g√∂r√ºyorsun!")
    elseif name == "Invisibility" then
        local character = player.Character
        if character then
            for _, part in pairs(character:GetDescendants()) do
                if part:IsA("BasePart") then
                    part.Transparency = 0.8
                end
            end
        end
        print("üëª Invisibility: AKTƒ∞F - G√∂r√ºnmez oldun!")
    elseif name == "FPS Boost" then
        print("‚ö° FPS Boost: AKTƒ∞F - Performans arttƒ±!")
    elseif name == "Admin Tools" then
        print("üîß Admin Tools: AKTƒ∞F - Admin ekipmanlarƒ± eklendi!")
    end
end

function deactivateFeature(name)
    if name == "Fly Mode" then
        flying = false
        if bodyVelocity then bodyVelocity:Destroy() bodyVelocity = nil end
        if bodyGyro then bodyGyro:Destroy() bodyGyro = nil end
        print("üöÄ Fly Mode: DEVRE DI≈ûI")
    elseif name == "NoClip Mode" then
        noclip = false
        print("üåÄ NoClip Mode: DEVRE DI≈ûI")
    elseif name == "God Mode" then
        godmode = false
        local character = player.Character
        if character then
            local humanoid = character:FindFirstChildOfClass("Humanoid")
            if humanoid then
                humanoid.MaxHealth = 100
                humanoid.Health = 100
            end
        end
        print("üõ°Ô∏è God Mode: DEVRE DI≈ûI")
    elseif name == "Speed Boost" then
        speedBoost = false
        local character = player.Character
        if character then
            local humanoid = character:FindFirstChildOfClass("Humanoid")
            if humanoid then
                humanoid.WalkSpeed = 16
            end
        end
        print("üí® Speed Boost: DEVRE DI≈ûI")
    elseif name == "Infinite Jump" then
        infiniteJump = false
        print("ü¶ò Infinite Jump: DEVRE DI≈ûI")
    elseif name == "High Jump" then
        jumpPower = false
        local character = player.Character
        if character then
            local humanoid = character:FindFirstChildOfClass("Humanoid")
            if humanoid then
                humanoid.JumpPower = 50
            end
        end
        print("üèÄ High Jump: DEVRE DI≈ûI")
    elseif name == "Player ESP" then
        for _, otherPlayer in pairs(Players:GetPlayers()) do
            if otherPlayer.Character then
                local highlight = otherPlayer.Character:FindFirstChild("Highlight")
                if highlight then
                    highlight:Destroy()
                end
            end
        end
        print("üëÅÔ∏è Player ESP: DEVRE DI≈ûI")
    elseif name == "Invisibility" then
        local character = player.Character
        if character then
            for _, part in pairs(character:GetDescendants()) do
                if part:IsA("BasePart") then
                    part.Transparency = 0
                end
            end
        end
        print("üëª Invisibility: DEVRE DI≈ûI")
    end
end

function switchTab(tabId)
    currentTab = tabId
    
    -- T√ºm sekmeleri gizle
    for _, tab in ipairs(tabs) do
        local frame = contentFrame:FindFirstChild(tab.id)
        if frame then
            frame.Visible = false
        end
        if tabButtons[tab.id] then
            TweenService:Create(tabButtons[tab.id], TweenInfo.new(0.3), {
                BackgroundColor3 = Colors.Secondary,
                TextColor3 = Colors.TextSecondary
            }):Play()
        end
    end
    
    -- Aktif sekme
    local activeFrame = contentFrame:FindFirstChild(tabId)
    if activeFrame then
        activeFrame.Visible = true
    end
    if tabButtons[tabId] then
        TweenService:Create(tabButtons[tabId], TweenInfo.new(0.3), {
            BackgroundColor3 = Colors.Accent,
            TextColor3 = Colors.TextPrimary
        }):Play()
    end
end

-- Modern sekme butonlarƒ± olu≈ütur
for i, tab in ipairs(tabs) do
    local tabButton = Instance.new("TextButton")
    tabButton.Size = UDim2.new(1, -15, 0, 52)
    tabButton.Position = UDim2.new(0, 7, 0, 15 + (i * 60))
    tabButton.BackgroundColor3 = Colors.Secondary
    tabButton.TextColor3 = Colors.TextSecondary
    tabButton.Text = "  " .. tab.icon .. "  " .. tab.name
    tabButton.TextSize = 14
    tabButton.Font = Enum.Font.Gotham
    tabButton.TextXAlignment = Enum.TextXAlignment.Left
    tabButton.AutoButtonColor = false
    tabButton.Parent = tabFrame
    
    local tabCorner = Instance.new("UICorner")
    tabCorner.CornerRadius = UDim.new(0, 10)
    tabCorner.Parent = tabButton
    
    local tabStroke = Instance.new("UIStroke")
    tabStroke.Color = Colors.Border
    tabStroke.Thickness = 1
    tabStroke.Parent = tabButton
    
    -- Hover efekti
    tabButton.MouseEnter:Connect(function()
        if currentTab ~= tab.id then
            TweenService:Create(tabButton, TweenInfo.new(0.2), {
                BackgroundColor3 = Color3.fromRGB(28, 33, 40)
            }):Play()
        end
    end)
    
    tabButton.MouseLeave:Connect(function()
        if currentTab ~= tab.id then
            TweenService:Create(tabButton, TweenInfo.new(0.2), {
                BackgroundColor3 = Colors.Secondary
            }):Play()
        end
    end)
    
    tabButtons[tab.id] = tabButton
    
    tabButton.MouseButton1Click:Connect(function()
        switchTab(tab.id)
    end)
end

