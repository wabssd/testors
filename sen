        TweenService:Create(executeButton, TweenInfo.new(0.2), {BackgroundColor3 = Color3.fromRGB(20, 120, 220)}):Play()
    end)
    
    executeButton.MouseLeave:Connect(function()
        TweenService:Create(executeButton, TweenInfo.new(0.2), {BackgroundColor3 = Colors.Accent}):Play()
    end)
    
    -- Click event - Admin komutlarƒ± i√ßin
    executeButton.MouseButton1Click:Connect(function()
        executeAdminCommand(name)
    end)
    
    return frame
end

function createTeleportButton(name, desc, parent)
    local frame = Instance.new("Frame")
