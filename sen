-- Modern fonksiyonlar
function createTabFrame(tabId)
    local frame = Instance.new("Frame")
    frame.Size = UDim2.new(1, 0, 1, 0)
    frame.Position = UDim2.new(0, 0, 0, 0)
    frame.BackgroundTransparency = 1
    frame.Visible = false
    frame.Parent = contentFrame
    frame.Name = tabId
    return frame
end

function createScrollFrame(parent)
    local scroll = Instance.new("ScrollingFrame")
    scroll.Size = UDim2.new(1, 0, 1, 0)
    scroll.Position = UDim2.new(0, 0, 0, 0)
    scroll.BackgroundTransparency = 1
    scroll.ScrollBarThickness = 6
    scroll.ScrollBarImageColor3 = Colors.Border
    scroll.ScrollBarImageTransparency = 0.5
    scroll.CanvasSize = UDim2.new(0, 0, 0, 0)
    scroll.AutomaticCanvasSize = Enum.AutomaticSize.Y
    scroll.Parent = parent
    
    local uiPadding = Instance.new("UIPadding")
    uiPadding.PaddingTop = UDim.new(0, 10)
    uiPadding.PaddingLeft = UDim.new(0, 10)
    uiPadding.Parent = scroll
    
    return scroll
end

function createToggleButton(name, desc, recommended, parent, category)
    local frame = Instance.new("Frame")
    frame.Size = UDim2.new(1, -20, 0, 90)
    frame.BackgroundColor3 = Colors.Secondary
    frame.BorderSizePixel = 0
    frame.Parent = parent
    frame.Name = name
    
    local corner = Instance.new("UICorner")
    corner.CornerRadius = UDim.new(0, 12)
    corner.Parent = frame
    
    local stroke = Instance.new("UIStroke")
    stroke.Color = Colors.Border
    stroke.Thickness = 1
    stroke.Parent = frame
    
    -- Hover efekti
    local hover = Instance.new("Frame")
    hover.Size = UDim2.new(1, 0, 1, 0)
    hover.BackgroundColor3 = Colors.TextPrimary
    hover.BackgroundTransparency = 0.95
    hover.BorderSizePixel = 0
    hover.Visible = false
    hover.Parent = frame
    
    local hoverCorner = Instance.new("UICorner")
    hoverCorner.CornerRadius = UDim.new(0, 12)
    hoverCorner.Parent = hover
    
    -- Ba≈ülƒ±k
    local titleLabel = Instance.new("TextLabel")
    titleLabel.Size = UDim2.new(0.7, 0, 0, 25)
    titleLabel.Position = UDim2.new(0, 15, 0, 12)
    titleLabel.BackgroundTransparency = 1
    titleLabel.TextColor3 = Colors.TextPrimary
    titleLabel.Text = name
    titleLabel.TextSize = 16
    titleLabel.Font = Enum.Font.GothamBold
    titleLabel.TextXAlignment = Enum.TextXAlignment.Left
    titleLabel.Parent = frame
    
    -- A√ßƒ±klama
    local descLabel = Instance.new("TextLabel")
    descLabel.Size = UDim2.new(0.7, -10, 0, 40)
    descLabel.Position = UDim2.new(0, 15, 0, 35)
    descLabel.BackgroundTransparency = 1
    descLabel.TextColor3 = Colors.TextSecondary
    descLabel.Text = desc
    descLabel.TextSize = 12
    descLabel.Font = Enum.Font.Gotham
    descLabel.TextXAlignment = Enum.TextXAlignment.Left
    descLabel.TextYAlignment = Enum.TextYAlignment.Top
    descLabel.TextWrapped = true
    descLabel.Parent = frame
    
    -- Recommended badge
    if recommended then
        local recommendedFrame = Instance.new("Frame")
        recommendedFrame.Size = UDim2.new(0, 100, 0, 22)
        recommendedFrame.Position = UDim2.new(0, 15, 0, 62)
        recommendedFrame.BackgroundColor3 = Colors.Success
        recommendedFrame.Parent = frame
        
        local recommendedCorner = Instance.new("UICorner")
        recommendedCorner.CornerRadius = UDim.new(0, 6)
        recommendedCorner.Parent = recommendedFrame
        
        local recommendedLabel = Instance.new("TextLabel")
        recommendedLabel.Size = UDim2.new(1, 0, 1, 0)
        recommendedLabel.BackgroundTransparency = 1
        recommendedLabel.TextColor3 = Colors.TextPrimary
        recommendedLabel.Text = "‚≠ê Tavsiye"
        recommendedLabel.TextSize = 10
        recommendedLabel.Font = Enum.Font.GothamBold
        recommendedLabel.Parent = recommendedFrame
    end
    
    -- Toggle Butonu
    local toggleButton = Instance.new("TextButton")
    toggleButton.Size = UDim2.new(0, 100, 0, 38)
    toggleButton.Position = UDim2.new(1, -120, 0, 25)
    toggleButton.BackgroundColor3 = Color3.fromRGB(48, 54, 61)
    toggleButton.TextColor3 = Colors.TextPrimary
    toggleButton.Text = "KAPALI"
    toggleButton.TextSize = 12
    toggleButton.Font = Enum.Font.GothamBold
    toggleButton.AutoButtonColor = false
    toggleButton.Parent = frame
    
    local toggleCorner = Instance.new("UICorner")
    toggleCorner.CornerRadius = UDim.new(0, 8)
    toggleCorner.Parent = toggleButton
    
    local toggleStroke = Instance.new("UIStroke")
    toggleStroke.Color = Colors.Border
    toggleStroke.Thickness = 2
    toggleStroke.Parent = toggleButton
    
    -- Hover efektleri
    frame.MouseEnter:Connect(function()
        hover.Visible = true
    end)
    
    frame.MouseLeave:Connect(function()
        hover.Visible = false
    end)
    
    toggleButton.MouseEnter:Connect(function()
        TweenService:Create(toggleButton, TweenInfo.new(0.2), {BackgroundColor3 = Color3.fromRGB(58, 64, 71)}):Play()
    end)
    
    toggleButton.MouseLeave:Connect(function()
        local data = toggleButtons[name]
        if data and not data.enabled then
            TweenService:Create(toggleButton, TweenInfo.new(0.2), {BackgroundColor3 = Color3.fromRGB(48, 54, 61)}):Play()
        end
    end)
    
    -- Toggle'ƒ± kaydet
    toggleButtons[name] = {
        button = toggleButton,
        enabled = false,
        category = category,
        name = name
    }
    
    -- Click event
    toggleButton.MouseButton1Click:Connect(function()
        toggleFeature(name)
    end)
    
    return frame
end

-- LAN Hƒ∞LE BUTONU FONKSƒ∞YONU
function createLanButton(name, desc, recommended, parent, category)
    local frame = Instance.new("Frame")
    frame.Size = UDim2.new(1, -20, 0, 90)
    frame.BackgroundColor3 = Colors.Secondary
    frame.BorderSizePixel = 0
    frame.Parent = parent
    frame.Name = name
    
    local corner = Instance.new("UICorner")
    corner.CornerRadius = UDim.new(0, 12)
    corner.Parent = frame
    
    local stroke = Instance.new("UIStroke")
    stroke.Color = Colors.Border
    stroke.Thickness = 1
    stroke.Parent = frame
    
    -- Hover efekti
    local hover = Instance.new("Frame")
    hover.Size = UDim2.new(1, 0, 1, 0)
    hover.BackgroundColor3 = Colors.TextPrimary
    hover.BackgroundTransparency = 0.95
    hover.BorderSizePixel = 0
    hover.Visible = false
    hover.Parent = frame
    
    local hoverCorner = Instance.new("UICorner")
    hoverCorner.CornerRadius = UDim.new(0, 12)
    hoverCorner.Parent = hover
    
    -- Ba≈ülƒ±k
    local titleLabel = Instance.new("TextLabel")
    titleLabel.Size = UDim2.new(0.7, 0, 0, 25)
    titleLabel.Position = UDim2.new(0, 15, 0, 12)
    titleLabel.BackgroundTransparency = 1
    titleLabel.TextColor3 = Colors.TextPrimary
    titleLabel.Text = name
    titleLabel.TextSize = 16
    titleLabel.Font = Enum.Font.GothamBold
    titleLabel.TextXAlignment = Enum.TextXAlignment.Left
    titleLabel.Parent = frame
    
    -- A√ßƒ±klama
    local descLabel = Instance.new("TextLabel")
    descLabel.Size = UDim2.new(0.7, -10, 0, 40)
    descLabel.Position = UDim2.new(0, 15, 0, 35)
    descLabel.BackgroundTransparency = 1
    descLabel.TextColor3 = Colors.TextSecondary
    descLabel.Text = desc
    descLabel.TextSize = 12
    descLabel.Font = Enum.Font.Gotham
    descLabel.TextXAlignment = Enum.TextXAlignment.Left
    descLabel.TextYAlignment = Enum.TextYAlignment.Top
    descLabel.TextWrapped = true
    descLabel.Parent = frame
    
    -- Warning badge (LAN hileleri i√ßin)
    if recommended then
        local warningFrame = Instance.new("Frame")
        warningFrame.Size = UDim2.new(0, 100, 0, 22)
        warningFrame.Position = UDim2.new(0, 15, 0, 62)
        warningFrame.BackgroundColor3 = Colors.Warning
        warningFrame.Parent = frame
        
        local warningCorner = Instance.new("UICorner")
        warningCorner.CornerRadius = UDim.new(0, 6)
        warningCorner.Parent = warningFrame
        
        local warningLabel = Instance.new("TextLabel")
        warningLabel.Size = UDim2.new(1, 0, 1, 0)
        warningLabel.BackgroundTransparency = 1
        warningLabel.TextColor3 = Colors.TextPrimary
        warningLabel.Text = "‚ö†Ô∏è LAN Hile"
        warningLabel.TextSize = 10
        warningLabel.Font = Enum.Font.GothamBold
        warningLabel.Parent = warningFrame
    end
    
    -- Execute Butonu (LAN hileleri i√ßin toggle deƒüil)
    local executeButton = Instance.new("TextButton")
    executeButton.Size = UDim2.new(0, 120, 0, 38)
    executeButton.Position = UDim2.new(1, -130, 0, 25)
    executeButton.BackgroundColor3 = Colors.Danger
    executeButton.TextColor3 = Colors.TextPrimary
    executeButton.Text = "üöÄ √áALI≈ûTIR"
    executeButton.TextSize = 12
    executeButton.Font = Enum.Font.GothamBold
    executeButton.AutoButtonColor = false
    executeButton.Parent = frame
    
    local executeCorner = Instance.new("UICorner")
    executeCorner.CornerRadius = UDim.new(0, 8)
    executeCorner.Parent = executeButton
    
    local executeStroke = Instance.new("UIStroke")
    executeStroke.Color = Color3.fromRGB(255, 100, 100)
    executeStroke.Thickness = 2
    executeStroke.Parent = executeButton
    
    -- Hover efektleri
    frame.MouseEnter:Connect(function()
        hover.Visible = true
    end)
    
    frame.MouseLeave:Connect(function()
        hover.Visible = false
    end)
    
    executeButton.MouseEnter:Connect(function()
        TweenService:Create(executeButton, TweenInfo.new(0.2), {BackgroundColor3 = Color3.fromRGB(255, 80, 80)}):Play()
    end)
    
    executeButton.MouseLeave:Connect(function()
        TweenService:Create(executeButton, TweenInfo.new(0.2), {BackgroundColor3 = Colors.Danger}):Play()
    end)
    
    -- Click event - LAN hileleri i√ßin
    executeButton.MouseButton1Click:Connect(function()
        executeLanFeature(name)
    end)
    
    return frame
end

-- ADMIN KOMUT BUTONU
function createCommandButton(name, desc, parent, category)
    local frame = Instance.new("Frame")
    frame.Size = UDim2.new(1, -20, 0, 90)
    frame.BackgroundColor3 = Colors.Secondary
    frame.BorderSizePixel = 0
    frame.Parent = parent
    frame.Name = name
    
    local corner = Instance.new("UICorner")
    corner.CornerRadius = UDim.new(0, 12)
    corner.Parent = frame
    
    local stroke = Instance.new("UIStroke")
    stroke.Color = Colors.Border
    stroke.Thickness = 1
    stroke.Parent = frame
    
    -- Hover efekti
    local hover = Instance.new("Frame")
    hover.Size = UDim2.new(1, 0, 1, 0)
    hover.BackgroundColor3 = Colors.TextPrimary
