local toggleButtons = {}

-- Fonksiyonlar
function createTabFrame(tabId)
    local frame = Instance.new("Frame")
    frame.Size = UDim2.new(1, 0, 1, 0)
    frame.Position = UDim2.new(0, 0, 0, 0)
    frame.BackgroundTransparency = 1
    frame.Visible = false
    frame.Parent = contentFrame
    frame.Name = tabId
    return frame
end

function createScrollFrame(parent)
    local scroll = Instance.new("ScrollingFrame")
    scroll.Size = UDim2.new(1, 0, 1, 0)
    scroll.Position = UDim2.new(0, 0, 0, 0)
    scroll.BackgroundTransparency = 1
    scroll.ScrollBarThickness = 6
    scroll.ScrollBarImageColor3 = Color3.fromRGB(48, 54, 61)
    scroll.CanvasSize = UDim2.new(0, 0, 0, 800)
    scroll.Parent = parent
    return scroll
end

function createToggleButton(name, desc, recommended, yPos, parent, category)
    local frame = Instance.new("Frame")
    frame.Size = UDim2.new(1, -20, 0, 80)
    frame.Position = UDim2.new(0, 10, 0, yPos)
    frame.BackgroundColor3 = Color3.fromRGB(22, 27, 34)
    frame.BorderSizePixel = 0
    frame.Parent = parent
    frame.Name = name
    
    local corner = Instance.new("UICorner")
    corner.CornerRadius = UDim.new(0, 8)
    corner.Parent = frame
    
    local stroke = Instance.new("UIStroke")
    stroke.Color = Color3.fromRGB(48, 54, 61)
    stroke.Thickness = 1
    stroke.Parent = frame
    
    -- Ba≈ülƒ±k
    local titleLabel = Instance.new("TextLabel")
    titleLabel.Size = UDim2.new(0.7, 0, 0, 25)
    titleLabel.Position = UDim2.new(0, 15, 0, 10)
    titleLabel.BackgroundTransparency = 1
    titleLabel.TextColor3 = Color3.new(1, 1, 1)
    titleLabel.Text = name
    titleLabel.TextSize = 16
    titleLabel.Font = Enum.Font.GothamBold
    titleLabel.TextXAlignment = Enum.TextXAlignment.Left
    titleLabel.Parent = frame
    
    -- A√ßƒ±klama
    local descLabel = Instance.new("TextLabel")
    descLabel.Size = UDim2.new(0.7, -10, 0, 35)
    descLabel.Position = UDim2.new(0, 15, 0, 35)
    descLabel.BackgroundTransparency = 1
    descLabel.TextColor3 = Color3.fromRGB(139, 148, 158)
    descLabel.Text = desc
    descLabel.TextSize = 12
    descLabel.Font = Enum.Font.Gotham
    descLabel.TextXAlignment = Enum.TextXAlignment.Left
    descLabel.TextYAlignment = Enum.TextYAlignment.Top
    descLabel.Parent = frame
    
    -- Recommended badge
    if recommended then
        local recommendedLabel = Instance.new("TextLabel")
        recommendedLabel.Size = UDim2.new(0, 90, 0, 20)
        recommendedLabel.Position = UDim2.new(0, 15, 0, 55)
        recommendedLabel.BackgroundColor3 = Color3.fromRGB(35, 134, 54)
        recommendedLabel.TextColor3 = Color3.new(1, 1, 1)
        recommendedLabel.Text = "Recommended"
        recommendedLabel.TextSize = 10
        recommendedLabel.Font = Enum.Font.GothamBold
        recommendedLabel.Parent = frame
        
        local recommendedCorner = Instance.new("UICorner")
        recommendedCorner.CornerRadius = UDim.new(0, 4)
        recommendedCorner.Parent = recommendedLabel
    end
    
    -- Toggle Butonu
    local toggleButton = Instance.new("TextButton")
    toggleButton.Size = UDim2.new(0, 100, 0, 35)
    toggleButton.Position = UDim2.new(1, -120, 0, 20)
    toggleButton.BackgroundColor3 = Color3.fromRGB(33, 38, 45)
    toggleButton.TextColor3 = Color3.new(1, 1, 1)
    toggleButton.Text = "OFF"
    toggleButton.TextSize = 12
    toggleButton.Font = Enum.Font.GothamBold
    toggleButton.Parent = frame
    
    local toggleCorner = Instance.new("UICorner")
    toggleCorner.CornerRadius = UDim.new(0, 6)
    toggleCorner.Parent = toggleButton
    
    local toggleStroke = Instance.new("UIStroke")
    toggleStroke.Color = Color3.fromRGB(48, 54, 61)
    toggleStroke.Thickness = 2
    toggleStroke.Parent = toggleButton
    
    -- Toggle'ƒ± kaydet
    toggleButtons[name] = {
        button = toggleButton,
        enabled = false,
        category = category,
        name = name
    }
    
    -- Click event
    toggleButton.MouseButton1Click:Connect(function()
        toggleFeature(name)
    end)
    
    return frame
end

function createTeleportButton(name, desc, yPos, parent)
    local frame = Instance.new("Frame")
    frame.Size = UDim2.new(1, -20, 0, 100)
    frame.Position = UDim2.new(0, 10, 0, yPos)
    frame.BackgroundColor3 = Color3.fromRGB(22, 27, 34)
    frame.BorderSizePixel = 0
    frame.Parent = parent
    
    local corner = Instance.new("UICorner")
    corner.CornerRadius = UDim.new(0, 8)
    corner.Parent = frame
    
    local stroke = Instance.new("UIStroke")
    stroke.Color = Color3.fromRGB(48, 54, 61)
    stroke.Thickness = 1
    stroke.Parent = frame
    
    -- Ba≈ülƒ±k
    local titleLabel = Instance.new("TextLabel")
    titleLabel.Size = UDim2.new(1, -20, 0, 25)
    titleLabel.Position = UDim2.new(0, 15, 0, 10)
    titleLabel.BackgroundTransparency = 1
    titleLabel.TextColor3 = Color3.new(1, 1, 1)
    titleLabel.Text = name
    titleLabel.TextSize = 16
    titleLabel.Font = Enum.Font.GothamBold
    titleLabel.TextXAlignment = Enum.TextXAlignment.Left
    titleLabel.Parent = frame
    
    -- A√ßƒ±klama
    local descLabel = Instance.new("TextLabel")
    descLabel.Size = UDim2.new(1, -20, 0, 20)
    descLabel.Position = UDim2.new(0, 15, 0, 35)
    descLabel.BackgroundTransparency = 1
    descLabel.TextColor3 = Color3.fromRGB(139, 148, 158)
    descLabel.Text = desc
    descLabel.TextSize = 12
    descLabel.Font = Enum.Font.Gotham
    descLabel.TextXAlignment = Enum.TextXAlignment.Left
    descLabel.Parent = frame
    
    -- Oyuncu adƒ± input
    local playerInput = Instance.new("TextBox")
    playerInput.Size = UDim2.new(0.6, 0, 0, 30)
    playerInput.Position = UDim2.new(0, 15, 0, 60)
    playerInput.BackgroundColor3 = Color3.fromRGB(33, 38, 45)
    playerInput.TextColor3 = Color3.new(1, 1, 1)
    playerInput.PlaceholderText = "Oyuncu adƒ± yaz..."
    playerInput.PlaceholderColor3 = Color3.fromRGB(139, 148, 158)
    playerInput.Text = ""
    playerInput.TextSize = 12
    playerInput.Font = Enum.Font.Gotham
    playerInput.Parent = frame
    
    local inputCorner = Instance.new("UICorner")
    inputCorner.CornerRadius = UDim.new(0, 6)
    inputCorner.Parent = playerInput
    
    local inputStroke = Instance.new("UIStroke")
    inputStroke.Color = Color3.fromRGB(48, 54, 61)
    inputStroke.Thickness = 2
    inputStroke.Parent = playerInput
    
    -- Teleport Butonu
    local teleportButton = Instance.new("TextButton")
    teleportButton.Size = UDim2.new(0, 120, 0, 30)
    teleportButton.Position = UDim2.new(1, -140, 0, 60)
    teleportButton.BackgroundColor3 = Color3.fromRGB(0, 100, 200)
    teleportButton.TextColor3 = Color3.new(1, 1, 1)
    teleportButton.Text = "üöÄ I≈üƒ±nlan"
    teleportButton.TextSize = 12
    teleportButton.Font = Enum.Font.GothamBold
    teleportButton.Parent = frame
    
    local buttonCorner = Instance.new("UICorner")
    buttonCorner.CornerRadius = UDim.new(0, 6)
    buttonCorner.Parent = teleportButton
    
    local buttonStroke = Instance.new("UIStroke")
    buttonStroke.Color = Color3.fromRGB(100, 150, 255)
    buttonStroke.Thickness = 2
    buttonStroke.Parent = teleportButton
    
    -- Teleport fonksiyonu
    teleportButton.MouseButton1Click:Connect(function()
        local targetName = playerInput.Text
        if targetName and targetName ~= "" then
            teleportToPlayer(targetName)
        else
            playerInput.PlaceholderText = "√ñnce oyuncu adƒ± yaz!"
            wait(1)
            playerInput.PlaceholderText = "Oyuncu adƒ± yaz..."
        end
    end)
    
    return frame
end

function teleportToPlayer(playerName)
    local targetPlayer = findPlayer(playerName)
    if targetPlayer and targetPlayer.Character then
        local targetRoot = targetPlayer.Character:FindFirstChild("HumanoidRootPart")
        local myCharacter = player.Character
        local myRoot = myCharacter and myCharacter:FindFirstChild("HumanoidRootPart")
        
        if targetRoot and myRoot then
            myRoot.CFrame = targetRoot.CFrame + Vector3.new(0, 3, 0)
            print("‚úÖ " .. targetPlayer.Name .. " adlƒ± oyuncuya ƒ±≈üƒ±nlanƒ±ldƒ±!")
        else
            print("‚ùå Hedef oyuncu bulunamadƒ±!")
        end
    else
        print("‚ùå Oyuncu bulunamadƒ±: " .. playerName)
    end
end

function findPlayer(name)
    name = name:lower()
    for _, plr in pairs(Players:GetPlayers()) do
        if plr.Name:lower():find(name) or plr.DisplayName:lower():find(name) then
            return plr
        end
    end
    return nil
end

function toggleFeature(name)
    local data = toggleButtons[name]
    if not data then return end
    
    data.enabled = not data.enabled
    
    if data.enabled then
        data.button.BackgroundColor3 = Color3.fromRGB(35, 134, 54) -- Ye≈üil
        data.button.Text = "ON"
        activateFeature(name)
    else
        data.button.BackgroundColor3 = Color3.fromRGB(33, 38, 45) -- Gri
        data.button.Text = "OFF"
        deactivateFeature(name)
    end
end

function activateFeature(name)
    if name == "Fly Mode" then
        flying = true
        local character = player.Character
        if character then
            local root = character:FindFirstChild("HumanoidRootPart")
            if root then
                bodyVelocity = Instance.new("BodyVelocity")
                bodyVelocity.Velocity = Vector3.new(0, 0, 0)
                bodyVelocity.MaxForce = Vector3.new(40000, 40000, 40000)
                bodyVelocity.Parent = root
                
                bodyGyro = Instance.new("BodyGyro")
                bodyGyro.MaxTorque = Vector3.new(50000, 50000, 50000)
                bodyGyro.P = 10000
                bodyGyro.D = 1000
                bodyGyro.CFrame = root.CFrame
                bodyGyro.Parent = root
            end
        end
    elseif name == "NoClip Mode" then
        noclip = true
    elseif name == "God Mode" then
        godmode = true
        local character = player.Character
        if character then
            local humanoid = character:FindFirstChildOfClass("Humanoid")
            if humanoid then
                humanoid.MaxHealth = math.huge
                humanoid.Health = math.huge
            end
        end
    elseif name == "Speed Boost" then
        local character = player.Character
        if character then
            local humanoid = character:FindFirstChildOfClass("Humanoid")
            if humanoid then
                humanoid.WalkSpeed = 50
            end
        end
    elseif name == "Player ESP" then
        for _, otherPlayer in pairs(Players:GetPlayers()) do
            if otherPlayer ~= player and otherPlayer.Character then
                local highlight = Instance.new("Highlight")
                highlight.FillColor = Color3.new(1, 0, 0)
                highlight.OutlineColor = Color3.new(1, 1, 0)
                highlight.FillTransparency = 0.5
                highlight.OutlineTransparency = 0
                highlight.Parent = otherPlayer.Character
            end
        end
    elseif name == "Invisibility" then
        local character = player.Character
        if character then
            for _, part in pairs(character:GetDescendants()) do
                if part:IsA("BasePart") then
                    part.Transparency = 0.8
                end
            end
        end
    end
end

function deactivateFeature(name)
    if name == "Fly Mode" then
        flying = false
        if bodyVelocity then bodyVelocity:Destroy() bodyVelocity = nil end
        if bodyGyro then bodyGyro:Destroy() bodyGyro = nil end
    elseif name == "NoClip Mode" then
        noclip = false
    elseif name == "God Mode" then
        godmode = false
        local character = player.Character
        if character then
            local humanoid = character:FindFirstChildOfClass("Humanoid")
            if humanoid then
                humanoid.MaxHealth = 100
                humanoid.Health = 100
            end
        end
    elseif name == "Speed Boost" then
        local character = player.Character
        if character then
            local humanoid = character:FindFirstChildOfClass("Humanoid")
            if humanoid then
                humanoid.WalkSpeed = 16
            end
        end
    elseif name == "Player ESP" then
        for _, otherPlayer in pairs(Players:GetPlayers()) do
            if otherPlayer.Character then
                local highlight = otherPlayer.Character:FindFirstChild("Highlight")
                if highlight then
                    highlight:Destroy()
                end
            end
        end
    elseif name == "Invisibility" then
        local character = player.Character
        if character then
            for _, part in pairs(character:GetDescendants()) do
                if part:IsA("BasePart") then
                    part.Transparency = 0
                end
            end
        end
    end
end

function switchTab(tabId)
    currentTab = tabId
    
    -- T√ºm sekmeleri gizle
    for _, tab in ipairs(tabs) do
        local frame = contentFrame:FindFirstChild(tab.id)
        if frame then
            frame.Visible = false
        end
        if tabButtons[tab.id] then
            tabButtons[tab.id].BackgroundColor3 = Color3.fromRGB(33, 38, 45)
            tabButtons[tab.id].TextColor3 = Color3.fromRGB(139, 148, 158)
        end
    end
    
    -- Aktif sekme
    local activeFrame = contentFrame:FindFirstChild(tabId)
    if activeFrame then
        activeFrame.Visible = true
    end
    if tabButtons[tabId] then
        tabButtons[tabId].BackgroundColor3 = Color3.fromRGB(0, 100, 200)
        tabButtons[tabId].TextColor3 = Color3.new(1, 1, 1)
    end
end

-- Sekmeleri olu≈ütur
for i, tab in ipairs(tabs) do
    local tabButton = Instance.new("TextButton")
    tabButton.Size = UDim2.new(1, -10, 0, 50)
    tabButton.Position = UDim2.new(0, 5, 0, 10 + (i * 55))
    tabButton.BackgroundColor3 = Color3.fromRGB(33, 38, 45)
    tabButton.TextColor3 = Color3.fromRGB(139, 148, 158)
    tabButton.Text = tab.name
    tabButton.TextSize = 14
    tabButton.Font = Enum.Font.Gotham
    tabButton.Parent = tabFrame
    
    local tabCorner = Instance.new("UICorner")
    tabCorner.CornerRadius = UDim.new(0, 8)
    tabCorner.Parent = tabButton
    
    tabButtons[tab.id] = tabButton
    
    tabButton.MouseButton1Click:Connect(function()
        switchTab(tab.id)
    end)
end

